{
  "easycom": {
    "autoscan": true,
    "custom": {
      // uni-ui 规则如下配置
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页"
      }
    }
  ],
  "subPackages": [
    {
      "root": "subpages",
      "pages": [
        {
          "path": "registration/registration_campus",
          "style": {
            "navigationBarTitleText": "预约挂号"
          }
        },
        {
          "path": "registration/registration_depart_list",
          "style": {
            "navigationBarTitleText": "选择科室"
          }
        },
        {
          "path": "registration/registration_doctor_list",
          "style": {
            "navigationBarTitleText": "选择医生"
          }
        },
        {
          "path": "notice/notice_index",
          "style": {
            "navigationBarTitleText": "须知"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTitleText": "脚手架模版项目",
    "navigationBarBackgroundColor": "#eee",
    "backgroundColor": "#fff",
    "h5": {
      "titleNView": false
    }
  }
}
